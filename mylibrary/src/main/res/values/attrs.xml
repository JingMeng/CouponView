<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CouponView">

        <attr name="drawType">
            <enum name="circle" value="0" />
            <enum name="oval" value="1" />
            <enum name="triangle" value="2" />
            <enum name="square" value="3" />
        </attr>


        <attr name="isDrawLeftSide" format="boolean" />
        <attr name="isDrawRightSide" format="boolean" />
        <attr name="isDrawTopSide" format="boolean" />
        <attr name="isDrawBottomSide" format="boolean" />

        <attr name="dashGap" format="dimension" />
        <attr name="dashWidth" format="dimension" />
        <attr name="bgc" format="color" />

    </declare-styleable>
</resources>